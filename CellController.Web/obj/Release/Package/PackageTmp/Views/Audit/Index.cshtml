
@{
    Layout = "~/Views/Shared/template/_Layout.cshtml";
}


@section content {

    <link href="~/Content/CustomPanel.css" rel="stylesheet" />
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-info panel-custom-myheading">
                <div class="panel-heading">
                    <h3 class="panel-title" id="titleView">Audit Logs</h3>
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-md-12" id="mainGridContainer">
                            <br />
                            <div class="jqxgrid jqxgrid-sortable jqxgrid-autoheight jqxgrid-enablehover jqxgrid-enableellipsis jqxgrid-pageable jqxgrid-virtualmode jqxgrid-pagesizeoptions jqxgrid-showstatusbar jqxgrid-toolbar jqxgrid-columnsresize"
                                 id="AuditGrid"
                                 grid-width="100"
                                 data-url="/Audit/GetLogs"
                                 grid-selection-mode="singlerow"
                                 grid-pagesizeoptions="20,25,30,50,100"
                                 grid-hide-columns="0"
                                 @*data-action-buttons="Edit,Delete"*@
                            >
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}

@section JavaScript {
    <script src="~/Scripts/pages/admin/audit.js"></script>
}

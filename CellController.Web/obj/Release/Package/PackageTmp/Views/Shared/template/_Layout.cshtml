                <ul class="menu accordion-menu">
                    @{
                        if (ViewBag.Menu != null)
                        {
                            List<CellController.Web.ViewModels.MenuObject> menu = ViewBag.Menu as List<CellController.Web.ViewModels.MenuObject>;
                            int menuId = 1;

                            string isMenu = "";
                            string menuStyle = "";
                            string ID = "";

                            try
                            {
                                isMenu = HttpContext.Current.Request.Cookies["isMenu"].Value.ToString();
                            }
                            catch
                            {
                                isMenu = "";
                            }

                            try
                            {
                                ID = HttpContext.Current.Request.Cookies["menuID"].Value.ToString();
                            }
                            catch
                            {
                                ID = "";
                            }

                            foreach (CellController.Web.ViewModels.MenuObject item in menu)
                            {
                                string _droplink = "";
                                string subMenuId = "subMenu" + menuId.ToString();
                                menuId++;
                                string style = "";

                                if (item.IsActive)
                                {
                                    _droplink = "active open";
                                }

                                if (item.Items != null)
                                {
                                    <li class="droplink @_droplink">
                                                        <a href="@item.URL" class="waves-effect waves-button"><span class="menu-icon glyphicon @item.Icon"></span><p> @item.Name </p><span class="arrow"></span></a>
                                                        
                                                        @if (isMenu == "" || isMenu.ToString().ToUpper() == "FALSE")
                                                        {
                                                            menuStyle = "display:none;";
                                                        }
                                                        else
                                                        {
                                                            if (ID == subMenuId)
                                                            {
                                                                menuStyle = "display:block;";
                                                            }
                                                            else
                                                            {
                                                                menuStyle = "display:none;";
                                                            }
                                                        }

                                                        <ul id="@subMenuId" class="sub-menu" style="@menuStyle">
                                                            @{
                                                                List<CellController.Web.ViewModels.MenuObject> submenu = item.Items;

                                                                foreach (CellController.Web.ViewModels.MenuObject si in submenu)
                                                                {
                                                                    string _droplink_subitem = "";
                                                                    if (si.IsActive)
                                                                    {
                                                                        _droplink_subitem = "active";
                                                                    }

                                                                    <li onclick="set('@subMenuId');processMenu();" class="@_droplink_subitem"><a href="@si.URL">@si.Name</a></li>
                                                                    
                                                                }
                                                            }
                                                        </ul>
                                    </li>                  
                                }
                                else
                                {
                                    //if (item.Name == "Track In")
                                    //{
                                    //     style = "color:#72e251;";
                                    //}
                                    //if (item.Name == "Track Out")
                                    //{
                                    //     style = "color:#eef442;";
                                    //}
                                    //if (item.Name == "Data Collection")
                                    //{
                                    //     style = "color:#f47741;";   
                                    //}
                                    //if (item.Name == "EProcedure")
                                    //{
                                    //    style = "color:#51d1e2;";
                                    //}
                                    //if (item.Name == "Engineering Mode")
                                    //{
                                    //     style = "color:#13f4ca;";
                                    //}

                                    <li onclick="reset();processMenu();" class="@_droplink"><a href="@item.URL" style="@style" class="waves-effect waves-button customNavBar"><span class="menu-icon glyphicon @item.Icon"></span><p>@item.Name</p></a></li>
                                }
                            }
                        }

                    }
                </ul>
            </div><!-- Page Sidebar Inner -->
        </div><!-- Page Sidebar -->
